    <class name="Book" table="books" schema="bls">
        <id name="bookid" column="bookid">
            <generator class="native"/>
        </id>
        <property name="bookCode" column="bookCode"/>
        <property name="bookName" column="bookName"/>
        <property name="bookType" column="bookType"/>
        <property name="author" column="author"/>
        <property name="publishPress" column="publishPress"/>
        <property name="publishDate" column="publishDate"/>
        <property name="isBorrow" column="isBorrow"/>
        <property name="createdBy" column="createdBy"/>
        <property name="creationTime" column="creationTime"/>
        <property name="lastUpdateTime" column="lastUpdateTime"/>

    </class>
</hibernate-mapping>